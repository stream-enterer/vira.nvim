{
  "_meta": {
    "description": "Maps JetBrains theme attributes to Neovim highlight groups",
    "version": "1.0",
    "notes": {
      "arrays": "First element is primary target, others are aliases/fallbacks",
      "special_values": {
        "fg": "Use FOREGROUND from attribute",
        "bg": "Use BACKGROUND from attribute",
        "sp": "Use EFFECT_COLOR from attribute",
        "bold": "FONT_TYPE contains 1",
        "italic": "FONT_TYPE contains 2",
        "underline": "EFFECT_TYPE 1",
        "undercurl": "EFFECT_TYPE 2",
        "strikethrough": "EFFECT_TYPE 3"
      }
    }
  },

